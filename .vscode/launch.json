    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Debug/GNU/Linux/XML2APK",
            "args": ["pubg443.zip.xml", "pubg444.zip.xml", "pubg445.zip.xml", "pubg446.zip.xml", "pubg447.zip.xml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Debug/GNU/Linux/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
